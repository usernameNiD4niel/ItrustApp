<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:paddingHorizontal="@dimen/_20sdp"
    android:paddingBottom="@dimen/_15sdp"
    xmlns:app="http://schemas.android.com/apk/res-auto">

    <TextView
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:text="@string/received_history"
        android:textStyle="bold"
        android:fontFamily="@font/poppins_medium"
        app:layout_constraintEnd_toEndOf="parent"
        android:textSize="@dimen/_16ssp"
        android:id="@+id/received_history_header_text"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintStart_toStartOf="parent"/>

    <TextView
        android:layout_width="0dp"
        android:fontFamily="@font/poppins_medium"
        android:id="@+id/received_history_header_content"
        android:layout_height="wrap_content"
        android:text="@string/this_track_all_the_residence_in_the_barangay_alawihao_when_receiving_the_medicines"
        app:layout_constraintTop_toBottomOf="@+id/received_history_header_text"
        app:layout_constraintStart_toStartOf="@+id/received_history_header_text"
        app:layout_constraintEnd_toEndOf="parent"/>

</androidx.constraintlayout.widget.ConstraintLayout>